#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/khalipa/Desktop/PLP_MERN/mern-bug-tracker/server/node_modules/.pnpm/md5-file@5.0.0/node_modules/md5-file/node_modules:/home/khalipa/Desktop/PLP_MERN/mern-bug-tracker/server/node_modules/.pnpm/md5-file@5.0.0/node_modules:/home/khalipa/Desktop/PLP_MERN/mern-bug-tracker/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/khalipa/Desktop/PLP_MERN/mern-bug-tracker/server/node_modules/.pnpm/md5-file@5.0.0/node_modules/md5-file/node_modules:/home/khalipa/Desktop/PLP_MERN/mern-bug-tracker/server/node_modules/.pnpm/md5-file@5.0.0/node_modules:/home/khalipa/Desktop/PLP_MERN/mern-bug-tracker/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../md5-file/cli.js" "$@"
else
  exec node  "$basedir/../md5-file/cli.js" "$@"
fi
